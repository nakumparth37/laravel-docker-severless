{
  "family": "laravel-php",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::136003615631:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::136003615631:role/ecsTaskRole",
  "enableExecuteCommand": true,
  "containerDefinitions": [
    {
      "name": "laravel-php",
      "image": "136003615631.dkr.ecr.ap-south-1.amazonaws.com/laravel-php:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/laravel-php",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "php"
        }
      }
    }
  ]
}
